{
    "es5": true,
    "browser": true,
    "devel": true,

    "globals": {
        "require": true,
        "define": true
    },

    "indent":         4,
    "maxcomplexity":  10,
    "maxdepth":       2,
    "maxlen":         1000,
    "maxparams":      3,
    "maxstatements":  30,
    "quotmark":       "single",

    "bitwise":  false,
    "noempty":  false,

    "camelcase":  true,
    "curly":      true,
    "eqeqeq":     true,
    "forin":      true,
    "immed":      true,
    "latedef":    true,
    "newcap":     true,
    "noarg":      true,
    "nonew":      true,
    "plusplus":   true,
    "strict":     true,
    "trailing":   true,
    "undef":      true,
    "unused":     true
}
